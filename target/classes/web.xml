<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>HDPAppStudio</display-name>
	<servlet>
		<description></description>
		<display-name>HDPAppStudio</display-name>
		<servlet-name>AppCreator</servlet-name>
		<servlet-class>com.hortonworks.digitalemil.hdpappstudio.web.AppCreator</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AppCreator</servlet-name>
		<url-pattern>/app/create</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UploadServlet</display-name>
		<servlet-name>UploadServlet</servlet-name>
		<servlet-class>com.hortonworks.digitalemil.hdpappstudio.web.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadServlet</servlet-name>
		<url-pattern>/app/upload</url-pattern>
	</servlet-mapping>
</web-app>